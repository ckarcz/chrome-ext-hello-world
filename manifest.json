{
    "manifest_version": 2,
    "name": "Test Extension",
    "description": "Following a Chrome extension tutorial.",
    "version": "1.0",
    "icons": {
        "16": "hello_16x16.png",
        "32": "hello_16x16.png",
        "48": "hello_16x16.png",
        "128": "hello_16x16.png"
    },
    "permissions": [
        "storage",
        "declarativeContent"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "hello.html",
        "default_icon": {
            "16": "hello_16x16.png",
            "32": "hello_16x16.png",
            "48": "hello_16x16.png",
            "128": "hello_16x16.png"
        }
    },
    "commands": {
        "_execute_page_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens extension popup."
        }
    }
}