{
    "name": "Test Extension",
    "description": "Following a Chrome extension tutorial.",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "16": "hello_16x16.png",
        "48": "hello_16x16.png",
        "128": "hello_16x16.png"
    },
    "browser_action": {
        "default_popup": "hello.html",
        "default_icon": "hello_16x16.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens hello.html"
        }
    }
}